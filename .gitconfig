[user]
	name = janwilamowski
	email = wilamowski.jan@gmail.com
[alias]
	st = status
	ci = commit
	co = checkout
	dt = difftool
	mt = mergetool -t meld
[diff]
	tool = meld
[difftool]
	prompt = false
[merge]
	tool = meld
[core]
	pager = less -+X -+F -x4
	whitespace = fix,cr-at-eol
	autocrlf = false
[push]
	default = upstream
[color "status"]
	changed = cyan
	untracked = dim
